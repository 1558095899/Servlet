<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>RegisterServer</servlet-name>
    <servlet-class>com.mvc.servlet.RegisterServer</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServer</servlet-name>
    <servlet-class>com.mvc.servlet.LoginServer</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>RegisterServer</servlet-name>
    <url-pattern>/registerServer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServer</servlet-name>
    <url-pattern>/loginServer</url-pattern>
  </servlet-mapping>

</web-app>